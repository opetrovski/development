        </operation>
        <operation name="getAccessibleServices">
            <input>
                <wsp:PolicyReference URI="#OrganizationalUnitServicePortSTSBinding_Input_Policy" />
            </input>
            <output>
                <wsp:PolicyReference URI="#OrganizationalUnitServicePortSTSBinding_Output_Policy" />
            </output>
        </operation>
        <operation name="addAccessibleServices">
            <input>
                <wsp:PolicyReference URI="#OrganizationalUnitServicePortSTSBinding_Input_Policy" />
            </input>
            <output>
                <wsp:PolicyReference URI="#OrganizationalUnitServicePortSTSBinding_Output_Policy" />
            </output>
        </operation>
        <operation name="revokeAccessibleServices">
            <input>
                <wsp:PolicyReference URI="#OrganizationalUnitServicePortSTSBinding_Input_Policy" />
            </input>
            <output>
                <wsp:PolicyReference URI="#OrganizationalUnitServicePortSTSBinding_Output_Policy" />
