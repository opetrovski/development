<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 9.0 EJB 3.0//EN" "../../../lib/dtds/sun-ejb-jar_3_0-1.dtd">
<sun-ejb-jar>
  <enterprise-beans>
    <unique-id>0</unique-id>
    <ejb>
      <ejb-name>AsynchronousOperationProxy</ejb-name>
      <webservice-endpoint>     
        <port-component-name>AsynchronousOperationProxy</port-component-name>
        <endpoint-address-uri>OperationService/AsynchronousOperationProxy</endpoint-address-uri>
        <login-config>
          <auth-method>CLIENT-CERT</auth-method>
        </login-config>
        <transport-guarantee>CONFIDENTIAL</transport-guarantee>
      </webservice-endpoint>
    </ejb>
    <ejb>
      <ejb-name>AsynchronousProvisioningProxy</ejb-name>
      <webservice-endpoint>
        <port-component-name>AsynchronousProvisioningProxy</port-component-name>
        <endpoint-address-uri>ProvisioningService/AsynchronousProvisioningProxy</endpoint-address-uri>
        <login-config>
          <auth-method>CLIENT-CERT</auth-method>
        </login-config>
        <transport-guarantee>CONFIDENTIAL</transport-guarantee>
      </webservice-endpoint>
    </ejb>
   
  </enterprise-beans>
</sun-ejb-jar>